[
  {
    "name" : "superset-image",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${AWS_LOGS_GROUP}",
        "awslogs-region": "${AWS_LOGS_REGION}",
        "awslogs-stream-prefix": "${AWS_LOGS_STREAM_PREFIX}"
      }
    },
    "portMappings": [
      {
        "hostPort": 8080,
        "containerPort": 8080,
        "protocol": "tcp"
      }
    ],
    "essential": true,
    "image": "${SUPERSET_IMAGE}",
    "environment": [
      {
        "name": "SUPERSET_SECRET_KEY",
        "value": "${SUPERSET_SECRET_KEY}"
      }
    ]
  }
]